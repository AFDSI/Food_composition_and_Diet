<rdf:Description rdf:about="http://vocabulary.semantic-web.at/wine_of_italy/499">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<foaf:page rdf:resource="http://en.wikipedia.org/wiki/Champagne_in_popular_culture"/>
	<skos:broader rdf:resource="http://vocabulary.semantic-web.at/wine_of_italy/76"/>
