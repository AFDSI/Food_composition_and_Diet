	<skos:altLabel xml:lang="en">Oak barks</skos:altLabel>
	<skos:altLabel xml:lang="en">Tan</skos:altLabel>
	<skos:altLabel xml:lang="en">Oak bark</skos:altLabel>
	<skos:altLabel xml:lang="en">Tan bark</skos:altLabel>
